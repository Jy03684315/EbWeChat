/* pages/display/display.wxss */
.image {
  margin-top: 10rpx;
  width: 740rpx;
  height: 150rpx;
}
.section{
   background:#CABBC7;
   margin:20rpx;
   padding:20rpx

}
.picker{
  margin: 20rpx 0 20rpx 77rpx;
  padding: 19rpx 26rpx;
  background-color: #FFFFFF;
}
.table {
  border: 0px solid #dadada;
  width:100%;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
  width:40%;
  text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.spin-article{
    width: 100%;
    height: 200px;
    position: relative;
    border: 0px solid #eee;
    text-align: center;
    box-sizing: border-box;
}
.weui-button{
  margin: 30rpx 0 30rpx 80rpx;
  color: #FFFFFF;
  background-color: #0099FF;
  border-color: #0099FF;
  width:600rpx;
  height:110rpx;
  border-radius: 30px;
  font-size:36rpx;
  line-height:50px;
}
.weui-cell {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.weui-cell_input {
  padding-top: 0;
  padding-bottom: 0;
  border: 1px solid #eee;
}
.weui-cell__bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.weui-input {
  height: 2.58823529em;
  min-height: 2.58823529em;
  line-height: 2.58823529em;
}
.weui-label {
  width: 105px;
  word-wrap: break-word;
  word-break: break-all;
}