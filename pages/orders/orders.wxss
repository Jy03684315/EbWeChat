/* pages/orders/orders.wxss */
.image {
  margin: 50rpx 0 50rpx 0;
  width: 100%;
  height: 130rpx;
}
.page-body{
  margin: 20rpx 10rpx 0 10rpx;
  background: #fff;
  border-radius: 6rpx;
  padding:20rpx 20rpx 20rpx 20rpx;
  margin-top:50rpx;
  position:relative;
  box-shadow:0 5px 5px rgba(0,0,0,.2);
}
.list{
  padding:0 0 0 0;
}
.title{
  background:#0099FF;
  text-align: center;
  color: #FFFFFF;
  line-height:20px;
  margin-bottom: -50rpx;
  height: 50rpx;
  font-size: 30rpx;
}
.weui-input {
  width: 100%;
  height: 2.58823529em;
  min-height: 2.58823529em;
  line-height: 2.58823529em;
  border-bottom: 1rpx solid #D9D9D9;
}
.weui-cells {
  position: relative;
  margin-top: 1.17647059em;
  background-color: #FFFFFF;
  line-height: 1.41176471;
  font-size: 17px;
}
.weui-cells_after-title {
  margin-top: 0;
}
.weui-cell {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.weui-cell_input {
  padding-top: 0;
  padding-bottom: 0;
}
.page-section{
  margin: 30rpx 0 30rpx 0;
}
.weui-button{
  margin: auto;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
  color: #FFFFFF;
  background-color: #0099FF;
  border-color: #0099FF;
  width:500rpx;
  height:80rpx;
  border-radius: 30px;
  font-size:30rpx;
  line-height:40px;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.spin-article{
    width: 100%;
    height: 200px;
    padding: 10px;
    position: relative;
    border: 1px solid #eee;
    text-align: center;
    box-sizing: border-box;
}
.caption-wrap{
  margin-bottom: 30px;
}
.caption-title{
  font-weight: bold;
  padding:20px 10px;
}
.collapse-item-title{
  font-size: 14px;
  color: #737373;
  font-weight: bold;
}
.collapse-item-content{
  font-size: 16px;
  color: #444444;
}
.searchButton{
  display:inline;
  font-size:30rpx;
}